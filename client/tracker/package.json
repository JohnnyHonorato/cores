{
    "name": "tracker",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "npm run serve:single-spa",
        "build": "ng build",
        "build:prod": "ng build --configuration=production --deploy-url https://tracker.virtus-way.org/",
        "build:homolog": "ng build --configuration=homolog --deploy-url https://demo-tracker.virtus-way.org/",
        "build:infra": "ng build --configuration=infra --deploy-url https://infra-tracker.virtus-way.org/",
        "build:dev": "ng build --configuration=dev --deploy-url https://dev-tracker.virtus-way.org/",
        "build:dev.local": "ng build --configuration=dev.local --deploy-url http://localhost:4203/",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:single-spa": "ng build --prod --deploy-url http://localhost:4203/",
        "serve:single-spa": "ng serve --disable-host-check --port 4203 --deploy-url http://localhost:4203/ --live-reload true"
    },
